public class HCF {
    public static void hcf(int a,int b){
        ;
        for(int h=a<b?a:b;h>=1;h--){
            if(a%h==0 && b%h==0){
                System.out.print(h);
                break;
            }
        }
    }
    public static void main(String[] args){
        hcf(5,24);
    }
}
