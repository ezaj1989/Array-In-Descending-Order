public class LCM {
    public static void lcm(int a,int b){
        int l=a>b?a:b;
        for(int i=l;i<=a*b;i++){
            if(i%a==0 && i%b==0){
                System.out.println(i);
                break;
            }
        }
    }
    public static void main(String[] args){
       lcm(10,15);
    }
}
